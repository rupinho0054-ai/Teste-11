local Players = game:GetService("Players")

local ReplicatedStorage = game:GetService("ReplicatedStorage")

local UserInputService = game:GetService("UserInputService")

local LocalPlayer = Players.LocalPlayer

local PlayerGui = LocalPlayer:WaitForChild("PlayerGui")

-- Comandos para spam (removido jail e ragdoll)

local COMMANDS = {"morph", "jumpscare", "tiny", "balloon", "rocket", "inverse"}

-- Criar GUI Ultra Compacta

local function CreateGUI()

    if PlayerGui:FindFirstChild("SpamAdminPanel") then

        PlayerGui:FindFirstChild("SpamAdminPanel"):Destroy()

    end

    

    local sg = Instance.new("ScreenGui")

    sg.Name = "SpamAdminPanel"

    sg.ResetOnSpawn = false

    sg.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

    

    -- Frame Principal PRETO E MENOR

    local frame = Instance.new("Frame")

    frame.Name = "MainFrame"

    frame.Size = UDim2.new(0, 160, 0, 240)

    frame.Position = UDim2.new(0.5, -80, 0.5, -120)

    frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)

    frame.BackgroundTransparency = 0.2

    frame.BorderSizePixel = 0

    frame.Active = true

    frame.Parent = sg

    

    Instance.new("UICorner", frame).CornerRadius = UDim.new(0, 12)

    

    -- Header PRETO

    local header = Instance.new("Frame")

    header.Name = "Header"

    header.Size = UDim2.new(1, 0, 0, 28)

    header.BackgroundColor3 = Color3.fromRGB(10, 10, 10)

    header.BackgroundTransparency = 0.1

    header.BorderSizePixel = 0

    header.Parent = frame

    

    Instance.new("UICorner", header).CornerRadius = UDim.new(0, 12)

    

    local title = Instance.new("TextLabel")

    title.Size = UDim2.new(1, -40, 0, 14)

    title.Position = UDim2.new(0, 5, 0, 2)

    title.BackgroundTransparency = 1

    title.Text = "Spam Panel"

    title.TextColor3 = Color3.new(1, 1, 1)

    title.TextSize = 10

    title.Font = Enum.Font.GothamBold

