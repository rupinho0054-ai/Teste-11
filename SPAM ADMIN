    creator.TextXAlignment = Enum.TextXAlignment.Left

    creator.Parent = header

    

    -- Bot√£o Fechar

    local close = Instance.new("TextButton")

    close.Name = "CloseBtn"

    close.Size = UDim2.new(0, 20, 0, 20)

    close.Position = UDim2.new(1, -23, 0, 4)

