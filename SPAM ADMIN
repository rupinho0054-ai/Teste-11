local gui, frame, scroll, closeBtn = CreateGUI()

-- Criar bot√£o de player com bot√£o SPAM ao lado

local function CreatePlayerButton(player)

    local container = Instance.new("Frame")

    container.Name = player.Name .. "_Container"

    container.Size = UDim2.new(1, -4, 0, 22)

    container.BackgroundTransparency = 1

    

    -- Nome do player PRETO

    local nameBtn = Instance.new("TextButton")

    nameBtn.Name = "NameBtn"

    nameBtn.Size = UDim2.new(1, -48, 1, 0)

    nameBtn.Position = UDim2.new(0, 0, 0, 0)

    nameBtn.BackgroundColor3 = Color3.fromRGB(20, 20, 20)

    nameBtn.Text = player.Name .. (player == LocalPlayer and " (Voc√™)" or "")

    nameBtn.TextColor3 = player == LocalPlayer and Color3.fromRGB(100, 200, 255) or Color3.new(1, 1, 1)

    nameBtn.TextSize = 9

    nameBtn.Font = Enum.Font.Gotham

    nameBtn.TextXAlignment = Enum.TextXAlignment.Left

    nameBtn.BorderSizePixel = 0

    nameBtn.Parent = container

    

    local namePadding = Instance.new("UIPadding")

    namePadding.PaddingLeft = UDim.new(0, 5)

    namePadding.Parent = nameBtn

    

    Instance.new("UICorner", nameBtn).CornerRadius = UDim.new(0, 8)

    

    -- Bot√£o SPAM ao lado com texto "SPAM"

    local spamBtn = Instance.new("TextButton")

    spamBtn.Name = "SpamBtn"

    spamBtn.Size = UDim2.new(0, 44, 1, 0)

    spamBtn.Position = UDim2.new(1, -44, 0, 0)

    spamBtn.BackgroundColor3 = Color3.fromRGB(30, 30, 30)

    spamBtn.Text = "SPAM"

    spamBtn.TextColor3 = Color3.new(1, 1, 1)

    spamBtn.TextSize = 8

    spamBtn.Font = Enum.Font.GothamBold

    spamBtn.BorderSizePixel = 0

    spamBtn.Parent = container

    

    Instance.new("UICorner", spamBtn).CornerRadius = UDim.new(0, 8)

    

    return container, nameBtn, spamBtn

end

-- Atualizar lista de players

local function UpdatePlayers()

    for _, child in ipairs(scroll:GetChildren()) do

        if child:IsA("Frame") then

            child:Destroy()

        end

    end

    

    local count = 0

    

    -- Adicionar LocalPlayer primeiro (voc√™)

    local allPlayers = {LocalPlayer}

    for _, player in ipairs(Players:GetPlayers()) do

        if player ~= LocalPlayer then

            table.insert(allPlayers, player)

        end

    end

    

    for _, player in ipairs(allPlayers) do

        local container, nameBtn, spamBtn = CreatePlayerButton(player)

        container.Parent = scroll

        count = count + 1

        

        -- Hover no nome

        nameBtn.MouseEnter:Connect(function()

            nameBtn.BackgroundColor3 = Color3.fromRGB(35, 35, 35)

        end)

        

        nameBtn.MouseLeave:Connect(function()

            nameBtn.BackgroundColor3 = Color3.fromRGB(20, 20, 20)

        end)

        

        -- Hover no bot√£o spam

        spamBtn.MouseEnter:Connect(function()

            spamBtn.BackgroundColor3 = Color3.fromRGB(50, 50, 50)

        end)

        

        spamBtn.MouseLeave:Connect(function()

            spamBtn.BackgroundColor3 = Color3.fromRGB(30, 30, 30)

        end)

        

        -- A√ß√£o do bot√£o SPAM (SEM DELAYS)

        local spamming = false

        spamBtn.MouseButton1Click:Connect(function()

            if spamming then return end

            spamming = true

            

            spamBtn.BackgroundColor3 = Color3.fromRGB(60, 60, 60)

            spamBtn.Text = "..."

            nameBtn.BackgroundColor3 = Color3.fromRGB(40, 40, 40)

            

            print("üöÄ Spammando:", player.Name)

            

            -- Executar comandos em coroutine SEM DELAYS

            coroutine.wrap(function()

                for _, cmd in ipairs(COMMANDS) do

                    if not player.Parent then break end

                    

                    pcall(function()

                        local Net = require(ReplicatedStorage.Packages.Net)

                        local Event = Net:RemoteEvent("352aad58-c786-4998-886b-3e4fa390721e")

                        Event:FireServer("78a772b6-9e1c-4827-ab8b-04a07838f298", player, cmd)

                    end)

                end

                

                -- Feedback visual instant√¢neo

                spamBtn.Text = "OK"

                spamBtn.BackgroundColor3 = Color3.fromRGB(30, 100, 30)

                nameBtn.BackgroundColor3 = Color3.fromRGB(20, 20, 20)

                

                spamBtn.Text = "SPAM"

                spamBtn.BackgroundColor3 = Color3.fromRGB(30, 30, 30)

                spamming = false

            end)()

        end)

    end

    

    scroll.CanvasSize = UDim2.new(0, 0, 0, count * 24)

end

-- Sistema de arrastar simplificado - APENAS UM TOQUE

local dragging = false

local dragInput

local dragStart

local startPos

local function update(input)

    if dragging then

        local delta = input.Position - dragStart

        frame.Position = UDim2.new(

            startPos.X.Scale,

            startPos.X.Offset + delta.X,

            startPos.Y.Scale,

            startPos.Y.Offset + delta.Y

        )

    end

end

frame.InputBegan:Connect(function(input)

    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then

        dragging = true

        dragStart = input.Position

        startPos = frame.Position

        

        input.Changed:Connect(function()

            if input.UserInputState == Enum.UserInputState.End then

                dragging = false

            end

        end)

    end

end)

frame.InputChanged:Connect(function(input)

    if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then

        dragInput = input

    end

end)

UserInputService.InputChanged:Connect(function(input)

    if input == dragInput and dragging then

        update(input)

    end

end)

-- Eventos

closeBtn.MouseButton1Click:Connect(function() gui:Destroy() end)

Players.PlayerAdded:Connect(UpdatePlayers)

Players.PlayerRemoving:Connect(UpdatePlayers)

-- Inicializar

print("‚úÖ Spam Panel - Ultra Compacto")

print("üë§ Usando como:", LocalPlayer.Name)

UpdatePlayers()
